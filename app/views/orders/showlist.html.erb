<p class="title_of_page">List Orders </p>
<% if @orders.delivered.count == 0 %>
  <p class="myorders_message message_myorders">
    No orders delivered in the specified dates. Check Pending orders to deliver orders
  </p>
<% else %>
  <%= render partial:"show-orderslist",locals: {
      orders_list:@orders.delivered,
      show_delivered_button:false,
    }
  %>
<% end %>
<form action="/menus" method="get">
  <input class="back_to_menu" type="submit" value="Back to menu">
</form>
